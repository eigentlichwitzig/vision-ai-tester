{
  "name": "vision-ai-tester",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "description": "Browser-based application for testing local Ollama multimodal models on construction documents",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
