    "vue-router": "^4.5.0",
    "pinia": "^2.3.0",
    "pinia-plugin-persistedstate": "^4.2.0",
    "dexie": "^4.0.11",
    "primevue": "^4.3.3",
    "primeicons": "^7.0.0",
    "@tato30/vue-pdf": "^1.12.4",
    "json-editor-vue": "^0.19.1",
    "jsondiffpatch": "^0.6.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "vite": "^6.0.7",
    "vue-tsc": "^2.1.10",
    "typescript": "~5.6.3",
    "@vue/tsconfig": "^0.7.0",
    "tailwindcss": "^3.4.17",
    "autoprefixer": "^10.4.20",
